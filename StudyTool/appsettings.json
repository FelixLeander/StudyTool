{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "MsSqlServerCardDatabaseConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=StudyTool;Trusted_Connection=True"
  },
  "AllowedHosts": "*"
}